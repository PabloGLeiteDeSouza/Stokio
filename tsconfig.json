{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./src/*"],
      "$types/*": ["./src/types/*"],
      "@components/*": ["./src/components/*"],
      "@screens": ["./src/screens"],
      "@app": ["./src/app"],
      "@utils/*": ["./src/utils/*"],
      "@hooks/*": ["./src/hooks/*"],
      "@screens/*": ["./src/app/screens/*"],
      "@services/*": ["./src/services/*"],
      "$assets/*": ["./assets/*"],
      "@constants/*": ["./src/constants/*"],
      "$providers/*": ["./src/providers/*"],
      "@classes/*": ["./src/classes/*"],
    }
  }
}
